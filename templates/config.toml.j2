user = "{{ solana_user }}"

[gossip]
    entrypoints = [
      {% for entrypoint in entrypoints %}
      "{{ entrypoint }}",
      {% endfor %}
    ]

[consensus]
    identity_path = "/etc/firedancer/identity.json"
    vote_account_path = "/etc/firedancer/vote.json"

    known_validators = [
        {% for known_validator in known_validators %}
        "{{ known_validator }}",
        {% endfor %}
    ]

[rpc]
    port = {{ rpc_port }}
{% if full_rpc_api %}
    full_api = true
{% endif %}
{% if private_rpc %}
    private = true
{% endif %}
[ledger]
    path = "{{ ledger }}"